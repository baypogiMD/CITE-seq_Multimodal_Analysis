import pandas as pd
from src.db_utils import get_engine

engine = get_engine()

cells = pd.read_csv("../data/raw/metadata.csv")
rna = pd.read_csv("../data/raw/train_rna.csv")
protein = pd.read_csv("../data/raw/train_protein.csv")

cells.to_sql("cells", engine, if_exists="replace", index=False)
rna.to_sql("rna_expression", engine, if_exists="replace", index=False)
protein.to_sql("protein_expression", engine, if_exists="replace", index=False)
